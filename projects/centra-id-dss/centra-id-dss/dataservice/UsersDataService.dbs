<data name="UsersDSS" serviceNamespace="centra-id.com" transports="https">
    <config id="default">
        <property name="carbon_datasource_name">CENTRA_ID</property>
    </config>
    <query id="update_all_query" useConfig="default">
        <sql>
           UPDATE :table SET :set WHERE :where
        </sql>
        <param name="table" sqlType="QUERY_STRING"/>
        <param name="set" sqlType="QUERY_STRING"/>
        <param name="where" sqlType="QUERY_STRING"/>
    </query>
    <query id="select_tasks_with_operation_query" useConfig="default">
        <sql>
            SELECT
                tp.ID ID,
                tp.UUID UUID,
                tp2.UUID UUID_PARENT
            FROM  SCH_CENTRA_ID.TAKS_PROVISIONING tp
            JOIN  SCH_CENTRA_ID.APPLICATIONS a ON tp.APPLICATION_ID=a.ID
            LEFT JOIN SCH_CENTRA_ID.TAKS_PROVISIONING tp2 ON tp2.OPERATION_PROVISIONING_ID=? AND tp2.APPLICATION_ID=a.PARENT_ID
            WHERE tp.ID=? AND tp.OPERATION_PROVISIONING_ID=?
        </sql>
        <param name="operationProvisioningId" ordinal="1" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <param name="taskProvisioningId" ordinal="2" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <param name="operationProvisioning2Id" ordinal="3" paramType="SCALAR" sqlType="INTEGER" type="IN"/>
        <result element="TaskProvisioning" rowName="User">
            <element column="ID" name="id" xsdType="xs:integer"/>
            <element column="UUID" name="uuid" xsdType="xs:string"/>
            <element column="UUID_PARENT" name="uuidParent" xsdType="xs:string"/>
        </result>
    </query>
