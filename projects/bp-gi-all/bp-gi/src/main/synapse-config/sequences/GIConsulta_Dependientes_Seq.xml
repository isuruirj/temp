<?xml version="1.0" encoding="UTF-8"?>
<sequence name="GIConsulta_Dependientes_Seq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log description="seq" level="custom">
        <property name="seq" value="-------------------- GIConsulta_Dependientes_Seq --------------------"/>
    </log>
    <filter regex=".*20.*" source="get-property('axis2', 'HTTP_SC')">
        <then>
            <filter regex=".*crearUsuario.*" source="get-property('SOAP_ACTION_REQUEST')">
                <then>
                    <property description="uri.var.resourcepath" name="uri.var.resourcepath" scope="default" type="STRING" value="RunDependencies"/>
                    <payloadFactory description="Request" media-type="json">
                        <format>{
					"runDependencies": {
					"uuid": "$1"
					}
					}</format>
                        <args>
                            <arg evaluator="xml" expression="get-property('uuid')"/>
                        </args>
                    </payloadFactory>
                    <sequence key="GITareas_Seq"/>
                </then>
                <else/>
            </filter>
        </then>
        <else/>
    </filter>
</sequence>
