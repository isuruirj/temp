<?xml version="1.0" encoding="UTF-8"?>
<proxy name="GIMedio" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <sequence key="GIMedio_Seq"/>
        </inSequence>
        <outSequence/>
        <faultSequence>
            <log description="seq" level="custom">
                <property name="seq" value="--------------------GIMedio ERROR  -------------------- "/>
                <property expression="get-property('GUID')" name="GUID"/>
                <property expression="get-property('Cod')" name="Cod"/>
            </log>
        </faultSequence>
    </target>
</proxy>
