<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="CheckOverdueTasksProxy" transports="https" startOnLoad="true" trace="disable">
	<target>
		<inSequence>
			<log level="custom" category="DEBUG">
				<property name="CheckOverdueTasksProxy" value="Inicio...." />
			</log>
			<property name="UserName" value="auto" scope="transport" />
			<property name="X-Forwarded-For" expression="get-property('SERVER_IP')" scope="transport" type="STRING" />
			<property name="actionCalled" value="TASK:/CheckOverdueTasksTask" />
			<sequence key="InBoundSeq" />
			<sequence key="CheckOverdueTasksSeq" />
			<sequence key="OutboundSeq" />
			<log level="custom" category="DEBUG">
				<property name="CheckOverdueTasksProxy" value="Fin...." />
			</log>
		</inSequence>
		<outSequence>
			<drop />
		</outSequence>
		<faultSequence />
	</target>
</proxy>