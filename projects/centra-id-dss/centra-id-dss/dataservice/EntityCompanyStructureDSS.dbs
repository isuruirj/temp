    <query id="dynamicQuery_companyStructure" useConfig="default">
        <sql>
           SELECT
               companyStructure.id,
               companyStructure.company_id,
               companyStructure.catalog_organizational_structure_id,
               companyStructure.parent_id,
               companyStructure.valid
           FROM sch_centra_id.COMPANIES_STRUCTURES companyStructure :innerjoinclause :whereclause ORDER BY :orderby :fromto
