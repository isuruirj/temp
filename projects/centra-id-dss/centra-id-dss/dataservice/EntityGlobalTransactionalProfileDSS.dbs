    </query>
    <!-- INSERT GlobalTransactionalProfile -->
    <query id="insertQuery_globalTransactionalProfile" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
        <sql>
           INSERT INTO sch_centra_id.GLOBAL_TRANSACTIONAL_PROFILES(
               date_created,
               date_deleted,
               user_created,
               user_deleted,
               valid,
               global_profile_id,
               transactional_profile_id
        ) VALUES(?,?,?,?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="dateCreated" ordinal="1" paramType="SCALAR" sqlType="TIMESTAMP" type="IN"/>
        <param name="dateDeleted" ordinal="2" paramType="SCALAR" sqlType="TIMESTAMP" type="IN"/>
        <param name="userCreated" ordinal="3" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="userDeleted" ordinal="4" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="valid" ordinal="5" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
        <param name="globalProfileId" ordinal="6" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="transactionalProfileId" ordinal="7" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
    </query>
    <!-- UPDATE GlobalTransactionalProfile -->
    <query id="updateQuery_globalTransactionalProfile" useConfig="default">
        <sql>
           UPDATE sch_centra_id.GLOBAL_TRANSACTIONAL_PROFILES SET
               date_created=?,
               date_deleted=?,
               user_created=?,
               user_deleted=?,
               global_profile_id=?,
               transactional_profile_id=?
