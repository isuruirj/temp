<?xml version="1.0" encoding="UTF-8"?>
<sequence name="SendNotificationSmsSeq" onError="FaultSeq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom" category="DEBUG">
        <property name="SendNotificationSmsSeq" value="Inicio.." />
    </log>
    <property action="remove" name="REST_URL_POSTFIX" scope="axis2" />
    <property name="service_ep" expression="get-property('ENDPOINT_NOTIFICACION_TCS')" />
    <header name="To" expression="get-property('service_ep')" />
    <call>
        <endpoint>
            <default />
        </endpoint>
    </call>
    <log level="custom" category="DEBUG">
        <property name="SendNotificationSmsSeq" value="Fin.." />
    </log>
</sequence>