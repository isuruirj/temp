    <!-- INSERT OperationProvisioning -->
    <query id="insertQuery_operationProvisioning" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
        <sql>
           INSERT INTO sch_centra_id.OPERATIONS_PROVISIONING(
               transactional_identity_id,
               global_profile_id,
               user_data_id,
               created_date,
               profile_start_date,
               profile_end_date,
               start_date,
               end_date,
               modify_data,
               profile_recalculation,
               status,
               valid
        ) VALUES(?,?,?,?,?,?,?,?,?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="transactionalIdentityId" ordinal="1" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="globalProfileId" ordinal="2" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="userDataId" ordinal="3" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="createdDate" ordinal="4" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="profileStartDate" ordinal="5" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="profileEndDate" ordinal="6" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="startDate" ordinal="7" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="endDate" ordinal="8" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="modifyData" ordinal="9" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
        <param name="profileRecalculation" ordinal="10" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
        <param name="status" ordinal="11" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="A"/>
        <param name="valid" ordinal="12" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
