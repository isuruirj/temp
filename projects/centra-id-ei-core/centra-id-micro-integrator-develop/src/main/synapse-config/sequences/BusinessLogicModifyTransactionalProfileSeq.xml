<?xml version="1.0" encoding="UTF-8"?>
<sequence name="BusinessLogicModifyTransactionalProfileSeq" onError="FaultSeq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom" category="DEBUG">
        <property name="BusinessLogicModifyTransactionalProfileSeq" value="Inicio.." />
        <property name="uri.var.transactionalProfileId" expression="//transactionalProfileId" />
    </log>
    <property name="uri.var.transactionalProfileId" expression="//transactionalProfileId" />
    <property name="type" value="=" />
    <filter regex="false" source="boolean($ctx:UserName)">
        <then>
            <property name="UserName" expression="get-property('transport','UserName')" />
        </then>
    </filter>
    <store messageStore="ProfilesSynchronizationStore" />
    <payloadFactory media-type="json">
        <format>
           {
                "result":  {
                "code" : 0,
                "message" : "Operation performed successfully"
                }
           }
        </format>
        <args />
    </payloadFactory>
    <property name="HTTP_SC" scope="axis2" type="STRING" value="200" />
    <property name="ContentType" value="application/json" scope="axis2" />
    <property name="messageType" value="application/json" scope="axis2" />
    <log level="custom" category="DEBUG">
        <property name="BusinessLogicModifyTransactionalProfileSeq" value="Fin.." />
    </log>
</sequence>