<?xml version="1.0" encoding="UTF-8"?>
<sequence name="ProvisioningConfigureTransactionalProfileSeq" onError="FaultSeq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom" category="DEBUG">
        <property name="ProvisioningConfigureTransactionalProfileSeq" value="Inicio.." />
    </log>
    <property name="uri.var.transactionalProfileId" expression="$ctx:taskProvisioningCreate//transactionalProfileId" />
    <call>
        <endpoint template="HTTPEndpointGetTemplate" uri="{uri.var.enterpriseIntegratorHost}/api.centraid/application/TransactionalProfile/{uri.var.transactionalProfileId}" />
    </call>
    <filter regex="200" source="get-property('axis2', 'HTTP_SC')">
        <then>
            <property name="transactionalProfile" expression="$body//*" scope="default" type="OM" />
        </then>
        <else>
            <property name="fault" value="ProvisioningSeq: Error get TransactionalProfile" />
            <sequence key="FaultSeq" />
        </else>
    </filter>
    <log level="custom" category="DEBUG">
        <property name="ProvisioningConfigureTransactionalProfileSeq" value="Fin.." />
    </log>
</sequence>