<?xml version="1.0" encoding="UTF-8"?>
<sequence name="GISoapVersion_Seq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <filter xpath="//*[local-name() = 'Envelope' and namespace-uri() = 'http://schemas.xmlsoap.org/soap/envelope/']">
        <then>
            <log level="custom">
                <property name="GISoapVersion_Seq" value="SOAP1.1"/>
            </log>
            <property name="SOAP_VERSION" scope="default" type="STRING" value="SOAP1.1"/>
            <property name="messageType" scope="axis2" type="STRING" value="text/xml"/>
            <property name="ContentType" scope="axis2" type="STRING" value="text/xml"/>
            <property name="CONTENT_TYPE" scope="axis2" type="STRING" value="text/xml"/>
        </then>
        <else>
            <log level="custom">
                <property name="GISoapVersion_Seq" value="SOAP1.2"/>
            </log>
            <property name="SOAP_VERSION" scope="default" type="STRING" value="SOAP1.2"/>
            <property name="messageType" scope="axis2" type="STRING" value="application/soap+xml"/>
            <property name="ContentType" scope="axis2" type="STRING" value="application/soap+xml"/>
            <property name="CONTENT_TYPE" scope="axis2" type="STRING" value="application/soap+xml"/>
        </else>
    </filter>
</sequence>
