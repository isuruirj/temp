<data name="application.global-transactional-profile.ds"
    serviceNamespace="centra-id.com"
    enableBatchRequests="false"
    enableBoxcarring="false"
    transports="https"
    disableLegacyBoxcarringMode="true">

    <config id="default">
        <property name="carbon_datasource_name">CENTRA_ID</property>
    </config>
    <!-- DYNAMIC QUERY GLOBALTRANSACTIONALPROFILE -->
    <query id="dynamicQuery_globalTransactionalProfile" useConfig="default">
        <sql>
           SELECT
               globalTransactionalProfile.id,
               globalTransactionalProfile.date_created,
               globalTransactionalProfile.date_deleted,
               globalTransactionalProfile.user_created,
               globalTransactionalProfile.user_deleted,
               globalTransactionalProfile.valid,
               globalTransactionalProfile.global_profile_id,
               globalTransactionalProfile.transactional_profile_id
           FROM sch_centra_id.GLOBAL_TRANSACTIONAL_PROFILES globalTransactionalProfile :innerjoinclause :whereclause ORDER BY :orderby :fromto
        </sql>
        <result element="globalTransactionalProfileList" rowName="globalTransactionalProfile">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="date_created" name="dateCreated" xsdType="xs:dateTime"/>
            <element column="date_deleted" name="dateDeleted" xsdType="xs:dateTime"/>
            <element column="user_created" name="userCreated" xsdType="xs:string"/>
            <element column="user_deleted" name="userDeleted" xsdType="xs:string"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
            <call-query href="queryReferenceById__globalTransactionalProfile_globalProfile">
                <with-param column="global_profile_id" name="id"/>
            </call-query>
            <call-query href="queryReferenceById__globalTransactionalProfile_transactionalProfile">
                <with-param column="transactional_profile_id" name="id"/>
            </call-query>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="fromto" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- DYNAMIC DISTINCT QUERY GLOBALTRANSACTIONALPROFILE GlobalProfile-->
    <query id="dynamicDistinctQuery_globalTransactionalProfile_globalProfile" useConfig="default">
        <sql>
           SELECT DISTINCT
               globalProfile.id,
