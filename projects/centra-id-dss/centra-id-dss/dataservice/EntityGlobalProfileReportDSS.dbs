               global_profile_name,
               global_profile_is_birth,
               global_profile_status,
               global_profile_ticket_number,
               global_profile_user_creation,
               global_profile_date_creation,
               global_profile_user_approval,
               global_profile_date_approval,
               global_profile_observation,
               global_profile_scope_rules_who,
               global_profile_scope_rules_that,
               valid
        ) VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="globalProfileCode" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="globalProfileName" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="globalProfileIsBirth" ordinal="3" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileStatus" ordinal="4" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileTicketNumber" ordinal="5" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileUserCreation" ordinal="6" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileDateCreation" ordinal="7" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileUserApproval" ordinal="8" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileDateApproval" ordinal="9" paramType="SCALAR" sqlType="TIMESTAMP" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileObservation" ordinal="10" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileScopeRulesWho" ordinal="11" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="globalProfileScopeRulesThat" ordinal="12" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="valid" ordinal="13" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
    </query>
    <!-- UPDATE GlobalProfileReport -->
    <query id="updateQuery_globalProfileReport" useConfig="default">
        <sql>
           UPDATE sch_centra_id.GLOBAL_PROFILES_REPORT_VW SET
               global_profile_code=?,
               global_profile_name=?,
               global_profile_is_birth=?,
               global_profile_status=?,
               global_profile_ticket_number=?,
               global_profile_user_creation=?,
               global_profile_date_creation=?,
               global_profile_user_approval=?,
               global_profile_date_approval=?,
               global_profile_observation=?,
               global_profile_scope_rules_who=?,
               global_profile_scope_rules_that=?
            WHERE id=?
        </sql>
        <param name="globalProfileCode" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="globalProfileName" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="globalProfileIsBirth" ordinal="3" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="#{NULL}"/>
