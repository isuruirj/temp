<?xml version="1.0" encoding="UTF-8"?>
<api context="/api.centraid/massiveprocess" name="MassiveProcessApi" version="1.0.0" version-type="context" xmlns="http://ws.apache.org/ns/synapse">
    <resource faultSequence="FaultSeq" methods="POST" uri-template="/Employees">
        <inSequence>
            <sequence key="InBoundSeq" />
            <sequence key="MassiveProcessUsersInitSeq" />
            <sequence key="OutboundSeq" />
            <respond />
        </inSequence>
        <outSequence />
    </resource>
    <resource methods="POST" uri-template="/MigrationEmployees">
        <inSequence>
            <sequence key="MigrationSendSeq" />
            <respond />
        </inSequence>
        <outSequence />
    </resource>
</api>