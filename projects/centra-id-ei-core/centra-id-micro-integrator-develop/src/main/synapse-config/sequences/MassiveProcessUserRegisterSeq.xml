<?xml version="1.0" encoding="UTF-8"?>
<sequence name="MassiveProcessUserRegisterSeq" onError="FaultMassiveProcessUsersSeq" trace="disable" xmlns="http://ws.apache.org/ns/synapse" xmlns:cen="centra-id.com">
    <log level="custom" category="DEBUG">
        <property name="MassiveProcessUserRegisterSeq" value="Inicio..." />
    </log>
    <payloadFactory media-type="xml">
        <format>
            <cen:_postcreatemassiveprocess xmlns:cen="centra-id.com">
                <cen:guid>$1</cen:guid>
                <cen:json>$2</cen:json>
                <cen:state>N</cen:state>
                <cen:result>P</cen:result>
                <cen:typeEvent>$3</cen:typeEvent>
                <cen:typeAction>$4</cen:typeAction>
                <cen:observations></cen:observations>
                <cen:codigoTrabajador>$5</cen:codigoTrabajador>
                <cen:apellidosEmpleado>$6</cen:apellidosEmpleado>
                <cen:nombresEmpleado>$7</cen:nombresEmpleado>
                <cen:identificacion>$8</cen:identificacion>
                <cen:identificacionSupervisor>$9</cen:identificacionSupervisor>
                <cen:nombreSupervisor>$10</cen:nombreSupervisor>
                <cen:codigoCompania>$11</cen:codigoCompania>
                <cen:nombreCompania>$12</cen:nombreCompania>
                <cen:codigoAgrupacion>$13</cen:codigoAgrupacion>
                <cen:nombreAgrupacion>$14</cen:nombreAgrupacion>
                <cen:codigoDivision>$15</cen:codigoDivision>
                <cen:nombreDivision>$16</cen:nombreDivision>
                <cen:codigoDepartamento>$17</cen:codigoDepartamento>
                <cen:nombreDepartamento>$18</cen:nombreDepartamento>
                <cen:codigoArea>$19</cen:codigoArea>
                <cen:nombreArea>$20</cen:nombreArea>
                <cen:codigoSucursal>$21</cen:codigoSucursal>
                <cen:nombreSucursal>$22</cen:nombreSucursal>
                <cen:codigoCentroCosto>$23</cen:codigoCentroCosto>
                <cen:nombreCentroCosto>$24</cen:nombreCentroCosto>
                <cen:nombreLocalidad>$25</cen:nombreLocalidad>
                <cen:nombreCargo>$26</cen:nombreCargo>
                <cen:codigoNivelJerarquico>$27</cen:codigoNivelJerarquico>
                <cen:nombreNivelJerarquico>$28</cen:nombreNivelJerarquico>
                <cen:numeroCuentaDeposito>$29</cen:numeroCuentaDeposito>
                <cen:fechaIngresoEmpleado>$30</cen:fechaIngresoEmpleado>
                <cen:fechaSalidaEmpleado>$31</cen:fechaSalidaEmpleado>
                <cen:estadoEmpleado>$32</cen:estadoEmpleado>
                <cen:codigoEmpleado>$33</cen:codigoEmpleado>
                <cen:tipoUsuario>$34</cen:tipoUsuario>
                <cen:subTipo>$35</cen:subTipo>
                <cen:fechaDatos>$36</cen:fechaDatos>
                <cen:celular>$37</cen:celular>
                <cen:modificaDatos>$38</cen:modificaDatos>
                <cen:recalculoPerfil>$39</cen:recalculoPerfil>
                <cen:username>$40</cen:username>
            </cen:_postcreatemassiveprocess>
        </format>
        <args>
            <arg evaluator="xml" expression="get-property('guid')')" />
            <arg evaluator="xml" expression="get-property('json')')" />
            <arg evaluator="xml" expression="get-property('typeEvent')')" />
            <arg evaluator="xml" expression="get-property('typeAction')')" />
            <arg evaluator="xml" expression="get-property('codigoTrabajador')" />
            <arg evaluator="xml" expression="get-property('apellidosEmpleado')" />
            <arg evaluator="xml" expression="get-property('nombresEmpleado')" />
            <arg evaluator="xml" expression="get-property('identificacion')" />
            <arg evaluator="xml" expression="get-property('identificacionSupervisor')" />
            <arg evaluator="xml" expression="get-property('nombreSupervisor')" />
            <arg evaluator="xml" expression="get-property('codigoCompania')" />
            <arg evaluator="xml" expression="get-property('nombreCompania')" />
            <arg evaluator="xml" expression="get-property('codigoAgrupacion')" />
            <arg evaluator="xml" expression="get-property('nombreAgrupacion')" />
            <arg evaluator="xml" expression="get-property('codigoDivision')" />
            <arg evaluator="xml" expression="get-property('nombreDivision')" />
            <arg evaluator="xml" expression="get-property('codigoDepartamento')" />
            <arg evaluator="xml" expression="get-property('nombreDepartamento')" />
            <arg evaluator="xml" expression="get-property('codigoArea')" />
            <arg evaluator="xml" expression="get-property('nombreArea')" />
            <arg evaluator="xml" expression="get-property('codigoSucursal')" />
            <arg evaluator="xml" expression="get-property('nombreSucursal')" />
            <arg evaluator="xml" expression="get-property('codigoCentroCosto')" />
            <arg evaluator="xml" expression="get-property('nombreCentroCosto')" />
            <arg evaluator="xml" expression="get-property('nombreLocalidad')" />
            <arg evaluator="xml" expression="get-property('nombreCargo')" />
            <arg evaluator="xml" expression="get-property('codigoNivelJerarquico')" />
            <arg evaluator="xml" expression="get-property('nombreNivelJerarquico')" />
            <arg evaluator="xml" expression="get-property('numeroCuentaDeposito')" />
            <arg evaluator="xml" expression="get-property('fechaIngresoEmpleado')" />
            <arg evaluator="xml" expression="get-property('fechaSalidaEmpleado')" />
            <arg evaluator="xml" value="ACTIVO" />
            <arg evaluator="xml" expression="get-property('codigoEmpleado')" />
            <arg evaluator="xml" expression="get-property('tipoUsuario')" />
            <arg evaluator="xml" expression="get-property('subTipo')" />
            <arg evaluator="xml" expression="get-property('fechaHora')" />
            <arg evaluator="xml" expression="get-property('celular')" />
            <arg evaluator="xml" expression="get-property('modifyData')" />
            <arg evaluator="xml" expression="get-property('profileRecalculation')" />
            <arg evaluator="xml" expression="get-property('username')" />
        </args>
    </payloadFactory>
    <property action="remove" name="REST_URL_POSTFIX" scope="axis2" />
    <call>
        <endpoint name="MassiveProcessDSS_Endpoint" template="HTTPEndpointPostTemplate" uri="{uri.var.dataServiceHost}/MassiveProcessDSS.HTTPEndpoint/createMassiveProcess" />
    </call>
    <property expression="//cen:Entry/cen:id" name="massiveProcessId" scope="default" type="STRING" />
    <log level="custom">
        <property name="MassiveProcessUserRegisterSeq" value="Fin..." />
    </log>
</sequence>