        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- INSERT CostCenter -->
    <query id="insertQuery_costCenter" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
        <sql>
           INSERT INTO sch_centra_id.COSTS_CENTERS(
               code,
               name,
               company_id,
               parent_id,
               status,
               valid,
               filter,
               city
        ) VALUES(?,?,?,?,?,?,?,?)
        </sql>
        <result element="GeneratedKeys" rowName="Entry" useColumnNumbers="true">
            <element column="1" name="id" xsdType="xs:long"/>
        </result>
        <param name="code" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="name" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="companyId" ordinal="3" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="parentId" ordinal="4" paramType="SCALAR" sqlType="BIGINT" type="IN" defaultValue="#{NULL}"/>
        <param name="status" ordinal="5" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="A"/>
        <param name="valid" ordinal="6" paramType="SCALAR" sqlType="INTEGER" type="IN" defaultValue="1"/>
        <param name="filter" ordinal="7" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
        <param name="city" ordinal="8" paramType="SCALAR" sqlType="STRING" type="IN" defaultValue="#{NULL}"/>
    </query>
    <!-- UPDATE CostCenter -->
    <query id="updateQuery_costCenter" useConfig="default">
        <sql>
           UPDATE sch_centra_id.COSTS_CENTERS SET
               code=?,
               name=?,
               company_id=?,
               parent_id=?,
               status=?,
               filter=?,
               city=?
            WHERE id=?
        </sql>
        <param name="code" ordinal="1" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="name" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN"/>
        <param name="companyId" ordinal="3" paramType="SCALAR" sqlType="BIGINT" type="IN"/>
        <param name="parentId" ordinal="4" paramType="SCALAR" sqlType="BIGINT" type="IN" defaultValue="#{NULL}"/>
