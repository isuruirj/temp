    <!-- DYNAMIC QUERY GLOBALPROFILEREPORT -->
    <query id="dynamicQuery_globalProfileReport" useConfig="default">
        <sql>
           SELECT
               globalProfileReport.id,
               globalProfileReport.global_profile_code,
               globalProfileReport.global_profile_name,
               globalProfileReport.global_profile_is_birth,
               globalProfileReport.global_profile_status,
               globalProfileReport.global_profile_ticket_number,
               globalProfileReport.global_profile_user_creation,
               globalProfileReport.global_profile_date_creation,
               globalProfileReport.global_profile_user_approval,
               globalProfileReport.global_profile_date_approval,
               globalProfileReport.global_profile_observation,
               globalProfileReport.global_profile_scope_rules_who,
               globalProfileReport.global_profile_scope_rules_that,
               globalProfileReport.global_profile_position_name_owner,
               globalProfileReport.valid
           FROM sch_centra_id.GLOBAL_PROFILES_REPORT_VW globalProfileReport :innerjoinclause :whereclause ORDER BY :orderby :fromto
        </sql>
        <result element="globalProfileReportList" rowName="globalProfileReport">
            <element column="id" name="id" xsdType="xs:integer"/>
            <element column="global_profile_code" name="globalProfileCode" xsdType="xs:string"/>
            <element column="global_profile_name" name="globalProfileName" xsdType="xs:string"/>
            <element column="global_profile_is_birth" name="globalProfileIsBirth" xsdType="xs:integer"/>
            <element column="global_profile_status" name="globalProfileStatus" xsdType="xs:string"/>
            <element column="global_profile_ticket_number" name="globalProfileTicketNumber" xsdType="xs:string"/>
            <element column="global_profile_user_creation" name="globalProfileUserCreation" xsdType="xs:string"/>
            <element column="global_profile_date_creation" name="globalProfileDateCreation" xsdType="xs:date"/>
            <element column="global_profile_user_approval" name="globalProfileUserApproval" xsdType="xs:string"/>
            <element column="global_profile_date_approval" name="globalProfileDateApproval" xsdType="xs:date"/>
            <element column="global_profile_observation" name="globalProfileObservation" xsdType="xs:string"/>
            <element column="global_profile_scope_rules_who" name="globalProfileScopeRulesWho" xsdType="xs:string"/>
            <element column="global_profile_scope_rules_that" name="globalProfileScopeRulesThat" xsdType="xs:string"/>
            <element column="global_profile_position_name_owner" name="globalProfilePositionNameOwner" xsdType="xs:string"/>
            <element column="valid" name="valid" xsdType="xs:integer"/>
        </result>
        <param name="whereclause" sqlType="QUERY_STRING"/>
        <param defaultValue="id" name="orderby" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="fromto" sqlType="QUERY_STRING"/>
        <param defaultValue="" name="innerjoinclause" sqlType="QUERY_STRING"/>
    </query>
    <!-- INSERT GlobalProfileReport -->
    <query id="insertQuery_globalProfileReport" returnGeneratedKeys="true" useConfig="default" keyColumns="1">
