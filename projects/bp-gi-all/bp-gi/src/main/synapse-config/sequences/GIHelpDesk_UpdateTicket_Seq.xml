<?xml version="1.0" encoding="UTF-8"?>
<sequence name="GIHelpDesk_UpdateTicket_Seq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log description="seq" level="custom">
        <property name="seq" value="-------------------- GIHelpDesk_UpdateTicket_Seq --------------------"/>
    </log>
    <payloadFactory description="Payload Update Ticket" media-type="xml">
        <format>
            <ns:UpdateIM_IncidentRequest xmlns:ns="http://schemas.hp.com/SM/7">
                <ns:model>
                    <ns:keys>
                        <ns:IncidentID type="String">$1</ns:IncidentID>
                    </ns:keys>
                    <ns:instance>
                        <ns:JournalUpdates type="Array">
                            <ns:JournalUpdates type="String">$2</ns:JournalUpdates>
                        </ns:JournalUpdates>
                    </ns:instance>
                    <ns:messages/>
                </ns:model>
            </ns:UpdateIM_IncidentRequest>
        </format>
        <args>
            <arg evaluator="xml" expression="get-property('IncidentID')"/>
            <arg value="prueba actualizacion"/>
        </args>
    </payloadFactory>
    <log description="Update Ticket" level="full"/>
    <header name="Action" scope="default" value="Update"/>
    <sequence key="GIHelpDesk_API_Seq"/>
</sequence>
