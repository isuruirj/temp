<?xml version="1.0" encoding="UTF-8"?>
<sequence name="MassiveProcessUsersGuiSeq" onError="FaultMassiveProcessUsersSeq" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom" category="DEBUG">
        <property name="MassiveProcessUsersGuiSeq" value="Inicio..." />
    </log>
    <property action="remove" name="REST_URL_POSTFIX" scope="axis2" />
    <payloadFactory media-type="xml">
        <format>
            <cen:_postcreateguid xmlns:cen="centra-id.com">
                <cen:event>$1</cen:event>
                <cen:guid>$2</cen:guid>
            </cen:_postcreateguid>
        </format>
        <args>
            <arg evaluator="xml" expression="get-property('event')" xmlns:cen="centra-id.com" />
            <arg evaluator="xml" expression="get-property('guid')" xmlns:cen="centra-id.com" />
        </args>
    </payloadFactory>
    <call>
        <endpoint name="GuidsDSS_Endpoint" template="HTTPEndpointPostTemplate" uri="{uri.var.dataServiceHost}/GuidsDSS.HTTPEndpoint/createGuid" />
    </call>
    <log level="custom" category="DEBUG">
        <property name="MassiveProcessUsersGuiSeq" value="Fin..." />
    </log>
</sequence>